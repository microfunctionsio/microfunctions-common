{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "importHelpers": true,
    "removeComments": false,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2017",
    "sourceMap": false,
    "allowJs": false,
    "strict": false,
    "strictNullChecks": false
  },
  "include": ["*.ts", "enums","interfaces"],
  "exclude": ["node_modules"]
}
